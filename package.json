{
  "name": "tic-tac-toe",
  "version": "0.0.1",
  "description": "Test assignment for a JavaScript developer job.",
  "main": "server/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "DEBUG=tic-tac-toe:* mocha test/server.spec.js && karma start --single-run",
    "start": "NODE_ENV=production node server/index.js",
    "build": "webpack --config=webpack.config.js",
    "release": "NODE_ENV=production npm run clean && npm run build",
    "watch": "DEBUG=tic-tac-toe:* concurrently --kill-others \"npm run build -- --watch\" \"nodemon server/index.js\"",
    "lint": "standard --verbose {client,server,test}/**/*.{js,jsx} *.js | snazzy",
    "clean": "rm -rf lib static/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baygeldin/tic-tac-toe.git"
  },
  "keywords": [
    "test",
    "javascript",
    "nodejs",
    "websockets",
    "webrtc",
    "webaudio",
    "react",
    "socket-io"
  ],
  "author": "Alexander Baygeldin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baygeldin/tic-tac-toe/issues"
  },
  "homepage": "https://github.com/baygeldin/tic-tac-toe#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "koa": "^2.2.0",
    "koa-static": "^3.0.0",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "concurrently": "^3.4.0",
    "debug": "^2.6.3",
    "enzyme": "^2.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "immutable": "^3.8.1",
    "karma": "^1.6.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-clipboard.js": "^1.0.1",
    "react-dom": "^15.4.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "sinon": "^2.1.0",
    "snazzy": "^6.0.0",
    "socket.io-client": "^1.7.3",
    "standard": "^9.0.2",
    "supertest": "^3.0.0",
    "webpack": "^2.3.2"
  }
}
